{
    "name": "@srascar/galaxy-migrations",
    "version": "1.0.5",
    "description": "Lightweight migration library in Node.js for NoSQL Document databases",
    "main": "index.js",
    "bin": {
        "galaxy-migrations": "./galaxy-migrations"
    },
    "scripts": {
        "build": "tsc",
        "build-dev": "tsc --watch",
        "check-format": "prettier --check --config .prettierrc.json \"src/**/*.ts\"",
        "lint": "tslint --project ./tsconfig.json",
        "prepack": "npm install && npm run build",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/srascar/galaxy-migrations.git"
    },
    "keywords": [
        "command line",
        "migration",
        "cosmosdb",
        "cosmos db",
        "documentdb",
        "document database",
        "azure",
        "nosql",
        "database"
    ],
    "author": "srascar",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/srascar/galaxy-migrations/issues"
    },
    "homepage": "https://github.com/srascar/galaxy-migrations#readme",
    "devDependencies": {
        "@types/jest": "^26.0",
        "@types/node": "^12.6.8",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "eslint": "^7.32.0",
        "jest": "^26.4",
        "prettier": "^2.2.1",
        "ts-jest": "^26.4",
        "tslint": "^5.18.0",
        "typescript": "^4.4.2"
    },
    "dependencies": {
        "@azure/cosmos": "^3.9.3",
        "commander": "^6.2.1",
        "mongoose": "^6.0.2",
        "yaml": "^1.10.0"
    }
}
